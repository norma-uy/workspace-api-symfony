{
    "name": "@perseus-uy/workspace",
    "version": "1.0.0",
    "description": "CI/CD tool that unifies and simplifies the development of software projects.",
    "directories": {
        "test": "tests"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/perseus-uy/workspace.git"
    },
    "author": "Perseus UY Team",
    "bugs": {
        "url": "https://github.com/perseus-uy/workspace/issues"
    },
    "homepage": "https://github.com/perseus-uy/workspace#readme",
    "license": "MIT",
    "private": true,
    "scripts": {
        "dev-server": "encore dev-server",
        "dev": "encore dev",
        "watch": "encore dev --watch",
        "build": "encore production --progress",
        "dev-server:docs": "encore dev-server --config-name docs-config",
        "dev-server:admin": "encore dev-server --config-name admin-config",
        "dev:docs": "encore dev --config-name docs-config",
        "dev:admin": "encore dev --config-name admin-config",
        "watch:docs": "encore dev --watch --config-name docs-config",
        "watch:admin": "encore dev --watch --config-name admin-config",
        "build:docs": "encore production --progress --config-name docs-config",
        "build:admin": "encore production --progress --config-name admin-config",
        "format": "prettier --write \"*.{js,jsx,ts,tsx,json,php}\" \"app/**/*.{js,jsx,ts,tsx,css,scss,json}\" \"templates/**/*.html.twig\" \"src/**/*.php\" \"tests/**/*.php\""
    },
    "dependencies": {
        "@popperjs/core": "^2.11.5",
        "bootstrap": "^5.1.3",
        "jquery": "^3.6.0"
    },
    "devDependencies": {
        "@prettier/plugin-php": "^0.18.9",
        "@symfony/webpack-encore": "^3.0.0",
        "autoprefixer": "^10.4.7",
        "core-js": "^3.23.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-n": "^15.0.0",
        "eslint-plugin-promise": "^6.0.0",
        "file-loader": "^6.0.0",
        "postcss-loader": "^7.0.0",
        "prettier": "^2.7.1",
        "prettier-plugin-twig-melody": "^0.4.6",
        "regenerator-runtime": "^0.13.9",
        "sass": "^1.53.0",
        "sass-loader": "^13.0.0",
        "stylelint": "^14.9.1",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-standard": "^26.0.0",
        "webpack-notifier": "^1.15.0"
    },
    "browserslist": [
        "defaults"
    ]
}
